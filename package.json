{
  "name": "iiko-landing",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --prefix-paths && rm -rf ./docs && mkdirp docs && cp -r ./public/* ./docs",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "copyright": "copyright-header --fix --copyrightHolder \"IceRock MAG Inc. Use of this source code is governed by the Apache 2.0 license.\" -i \"src|dist|docs\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@types/body-scroll-lock": "^2.6.1",
    "@types/classnames": "^2.2.10",
    "@types/flat": "^5.0.0",
    "@types/react-helmet": "^5.0.15",
    "@types/react-input-mask": "^2.0.5",
    "@types/validator": "^13.0.0",
    "axios": "^0.21.1",
    "body-scroll-lock": "^3.0.2",
    "classnames": "^2.2.6",
    "date-fns": "^2.12.0",
    "fs-extra": "^9.0.0",
    "gatsby": "^2.20.12",
    "gatsby-plugin-react-helmet": "^3.2.3",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-sass-resources": "^2.0.0",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-remark-images": "^3.2.2",
    "gatsby-source-filesystem": "^2.2.2",
    "i18next": "^19.4.2",
    "i18next-browser-languagedetector": "^4.1.0",
    "i18next-xhr-backend": "^3.2.2",
    "inputmask": "^5.0.3",
    "node-sass": "^4.13.1",
    "query-string": "^6.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-i18next": "^10.0.0",
    "react-input-mask": "^2.0.4",
    "react-intl": "^4.4.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "copyright-header": "^0.4.6",
    "prettier": "2.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
